<%@ Page Language="C#" AutoEventWireup="true" CodeFile="Login.aspx.cs" Inherits="Login" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>Login</title>
    <style type="text/css">
        .style1
        {
            width: 445px;
            height: 285px;
        }
        .style11
        {
            height: 76px;
        }
        .style12
        {
            height: 28px;
        }
        .style14
        {
            width: 119px;
            height: 28px;
        }
        .style15
        {
            height: 23px;
        }
        .style16
        {
            width: 100%;
            height: 600px;
        }
    </style>
</head>
<body>
    <form id="form1" runat="server">
    <div align="left" style="vertical-align: middle">
        <table cellpadding="0" cellspacing="0" class="style16" style="width: 100%; height: 350px">
            <tr>
                <td align="center" valign="middle">
                    <table align="center" class="style1" style="background-image: url('Imagem/Login.jpg');
                        background-repeat: no-repeat; width: 345px; height: 220px;">
                        <tr>
                            <td class="style11" colspan="2" valign="top">
                                &nbsp;&nbsp;&nbsp;&nbsp;
                            </td>
                        </tr>
                        <tr>
                            <td align="right" class="style14">
                                <asp:Label ID="lblLogin" runat="server" Font-Bold="True" Font-Names="Verdana" Font-Size="Small"
                                    Text="Usuário : " ForeColor="#003399"></asp:Label>
                            </td>
                            <td class="style12">
                                <asp:TextBox ID="TextBox1" runat="server" Font-Names="Verdana" Font-Size="Small"
                                    Width="145px" BorderStyle="Solid" BorderWidth="1px" BorderColor="Black"></asp:TextBox>
                            </td>
                        </tr>
                        <tr>
                            <td align="right" class="style14">
                                <asp:Label ID="lblSenha" runat="server" Font-Bold="True" Font-Names="Verdana" Font-Size="Small"
                                    Text="Senha : " ForeColor="#003399"></asp:Label>
                            </td>
                            <td class="style12">
                                <asp:TextBox ID="TextBox2" runat="server" Font-Names="Verdana" Font-Size="Small"
                                    TextMode="Password" BorderStyle="Solid" BorderWidth="1px" Width="145px" BorderColor="Black"></asp:TextBox>
                            </td>
                        </tr>
                        <tr>
                            <td align="center" class="style15" colspan="2">
                                <asp:Label ID="lblMsg" runat="server" Font-Names="Verdana" Font-Size="X-Small" ForeColor="Red"
                                    Width="157px" Visible="False"></asp:Label>
                            </td>
                        </tr>
                        <tr>
                            <td align="center" class="style12" colspan="2">
                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                <asp:Button ID="btnOk" runat="server" Font-Names="Verdana" Font-Size="10px" Text="Ok"
                                    Width="85px" OnClick="btnOk_Click" BackColor="White" BorderStyle="Solid" BorderWidth="1px"
                                    Height="21px" BorderColor="Black" />
                            </td>
                        </tr>
                        <tr>
                            <td align="center" colspan="2">
                                &nbsp;
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
        <br />
        <br />
    </div>
    </form>
</body>
</html>
